{
    "name": "trenergy-wallet/trenergy_main_api",
    "description": "Trenergy API client for Laravel",
    "type": "library",
    "license": "MIT",
    "prefer-stable": true,
    "authors": [{
        "name": "support-tr.energy",
        "email": "support@tr.energy"
    }],
    "autoload": {
        "psr-4": {
            "Apd\\Trenergy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Apd\\Trenergy\\Providers\\TrenergyServiceProvider"
            ],
            "aliases": {
                "Trenergy": "Apd\\Trenergy\\Facades\\Trenergy"
            }
        }
    },
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "guzzlehttp/guzzle": "^7.0"
    }
}